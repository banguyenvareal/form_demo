<link href="https://cdn.jsdelivr.net/npm/select2@4.0.11/dist/css/select2.min.css" rel="stylesheet" />
<script src="https://cdn.jsdelivr.net/npm/select2@4.0.11/dist/js/select2.min.js"></script>
<% if !user_signed_in? %>
  <%= link_to "Sign in with google", user_google_oauth2_omniauth_authorize_path %> <br>
<% else %>
<br>
  <% @bank_accounts.each do |bank_account| %>
    <%= link_to bank_account.name, bank_account_path(bank_account) %>
    <br>
  <% end %>
<%= link_to 'Create new bank account', new_bank_account_path %>
<br>
<%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% end %>
