<% if !user_signed_in? %>
  <%= link_to "Sign in with google", user_google_oauth2_omniauth_authorize_path %> <br>

<% else %>
  <%= link_to 'Withdraw', new_withdraw_user_path(current_user) %>
  <br>
   <%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% end %>
<br>
<%= link_to 'Create new bank account', new_bank_account_path %>
<br>
<% if @bank_accounts %>
  <% @bank_accounts.each do |bank_account| %>
  <%= link_to bank_account.name, bank_account_path(bank_account), remote: true %>
  <br>
  <% end %>
<% end %>
<br>
<div id="target-for-change">
  Now you see "ME"!
</div>
