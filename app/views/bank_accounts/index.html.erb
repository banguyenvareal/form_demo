<% if !user_signed_in? %>
  <%= link_to "Sign in with google", user_google_oauth2_omniauth_authorize_path %> <br>

<% else %>
  <%= link_to 'Withdraw', withdraw_user_path(current_user), remote: true %>
  <br>
   <%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% end %>
<br>
<%= link_to 'Create new bank account', new_bank_account_path %>
<% if @bank_accounts %>
  <% @bank_accounts.each do |bank_account| %>
    <p><%= bank_account.name %></p>
  <% end %>
<% end %>
<br>
